urls-source "freshrss"
freshrss-url "https://feed.unazoomer.com/api/greader.php"
freshrss-login "xxxxxxxxxxxxxxx"
freshrss-password "xxxxxxxxxxxxxxx"

max-items 100
bind-key j down
bind-key k up
bind-key l open
bind-key h quit
bind-key j next articlelist
bind-key k prev articlelist
bind-key ^D pagedown
bind-key ^U pageup

# bind-key U show-urls
unbind-key ENTER
browser ~/.config/newsboat/linkhandler.sh

include ~/.config/newsboat/themes/dark
